  text-align: left;
  font-size: 18px;
  padding: 0px 11%;
}

.pay-later{

  font-size: 18px;
  font-family: Helvetica Neue, Helvetica, Arial;
  font-weight: 200;

  input{
    width: 18px;
    height: 18px;
