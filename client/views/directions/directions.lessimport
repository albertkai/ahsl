.wait-for-pay{
  position: fixed;
  top: 30px;
  right: 30px;
  opacity: 0;
  visibility: hidden;
  -webkit-transition: all .4s;
  -moz-transition: all .4s;
  -ms-transition: all .4s;
  -o-transition: all .4s;
  transition: all .4s;
  width: 340px;
  height: 120px;
  text-align: center;
  background: white;
  border: 3px solid black;
  z-index: 9999;
  &._visible{
    opacity: 1;
    visibility: visible;
  }

}

.packs{

  .pack{
    .make-md-column(6);
    margin-bottom: 20px;
    padding: 10px;
    .note{
      font-size: 22px;
      color: @brand-primary;
      font-family: @font-family-serif;
    }
    &.full-width{
      .wrap{
        padding-bottom: 30px;
      }
      .make-md-column(12);
      .row{
        margin-top: 8px;
        >div:nth-child(1){
          button{
            width: 94%;
          }
        }
      }
      .actual{
        font-size: 50px !important;
      }
      img{
        width: 94%;
        margin: 3%;
      }
    }
    &:nth-child(1){
      h2, .discount{
        background: @gray;
        @media screen and (max-width: @screen-sm-max) {
          color: @gray;
          background-color: #e9e9e9;
          text-shadow: 0 0 0 #fff !important;
          span{
            text-shadow: 0 0 0 #fff;
          }
        }
      }
    }
    &:nth-child(2){
      h2, .discount{
        background: @brand-primary;
        @media screen and (max-width: @screen-sm-max) {
          color: @brand-primary;
          background-color: #e9e9e9;
          text-shadow: 0 0 0 #fff !important;
          span{
            text-shadow: 0 0 0 #fff;
          }
        }
      }
    }
    //&:nth-child(3){
    //  h2, .discount{
    //    background-image: url(/img/silver-bg.jpg);
    //  }
    //}
    &:nth-child(4){
      h2, .discount{
        background-image: url(/img/violet-satin-bg.jpg);
      }
    }
    .wrap{
      box-shadow: 0 0 3px rgba(0, 0, 0, .3);
      padding: 8px;
      transition: all .4s ease;
      -webkit-transition: all .4s ease;
      -moz-transition: all .4s ease;
      &:hover{
        box-shadow: 0 0 10px rgba(0, 0, 0, .3);
      }
    }
    @media screen and (max-width: @screen-sm-max) {
      padding: 18px;
      .wrap{
        box-shadow: 0 0 30px rgba(0, 0, 0, .3);
        button{
          box-shadow: 0 0 20px rgba(0, 0, 0, .6);
          background: @brand-primary;
          color: white;
        }
      }
    }
    h2{
      margin: 0;
      padding: 12px 0;
      color: white;
      -webkit-background-size: cover;
      background-size: cover;
      text-shadow: 0 0 3px rgba(0, 0, 0, .7);
    }

    .new-price{
      text-align: center;
      margin-top: 30px;
      h5{
        margin: 0;
        font-family: @font-family-serif;
      }
      p{
        font-size: 18px;
        font-family: @font-family-base;
        color: @gray-light;
        span{
          color: @gray-light;
          font-size: 24px;
          font-family: @font-family-serif;
          text-decoration: line-through;
        }
        .actual{
          font-size: 36px;
          color: @brand-primary;
          text-decoration: none;
        }
      }
    }

    hr{
      margin: 20px 0;
    }

    .price{
      //background: @brand-primary;
      color: white;
      font-size: 33px;
      padding: 12px 0;
      -webkit-background-size: cover;
      background-size: cover;
      display: flex;
      span{
        display: block;
        text-shadow: 0 0 3px rgba(0, 0, 0, .7);
        &:nth-child(1){
          color: white;
          font-size: 18px;
          font-family: @font-family-base;
        }
        &:nth-child(2){
          font-family: @font-family-serif;
          font-size: 45px;
        }
        &:nth-child(3){
          font-family: @font-family-serif;
          font-size: 28px;
          opacity: .8;
          text-decoration: line-through;
        }
      }
    }

    h4{
      text-align: center;
      font-size: 18px;
      margin: 20px;
    }
    li{
      font-family: @font-family-serif;
      font-size: 18px;
      font-weight: 300;
      text-align: center;
      margin-bottom: 12px;
      cursor: pointer;
      &:hover{
        color: @brand-primary;
      }
      strong{
        color: @brand-primary;
        font-weight: 400;
      }
    }
    ul{
      margin: 0;
      padding: 0;
      list-style: none;
      margin-top: 30px;
      >div{
        text-align: center;
        margin-bottom: 12px;
        >div:nth-child(1){
          font-family: @font-family-serif;
          font-size: 18px;
          font-weight: 300;
        }
        >div:nth-child(2){
          font-size: 16px;
          color: @gray-light;
        }
      }
    }
    .discount-cont{
      >div{
        &:nth-child(1){
          .make-md-column(4);
        }
        &:nth-child(2){
          .row;
          .make-md-column(8);
          text-align: center;
          >div{
            .make-md-column(3);
            .make-sm-column(3);
            .make-xs-column(3);
            >span{
              display: block;
              &:nth-child(1){
                font-size: 30px;
                font-family: @font-family-serif;
                color: @brand-primary;
              }
              &:nth-child(2){
                position: relative;
                top: -10px;
                font-size: 18px;
                color: @gray-light;
              }
            }
          }
        }
      }
    }
    .discount{
      text-align: right;
      color: white;
      width: 120px;
      height: 120px;
      border-radius: 50%;
      background-color: black;
      text-align: center;
      -webkit-background-size: cover;
      background-size: cover;
      @media screen and (max-width: @screen-sm-max) {
        margin: 0 auto;
      }
      span{
        display: block;
        text-shadow: 0 0 3px rgba(0, 0, 0, .7);
      }
      span:nth-child(1){
        font-size: 22px;
        position: relative;
        top: 10px;
      }
      span:nth-child(2){
        font-size: 48px;
        font-weight: 400;
      }
    }
    .lead{

      display: block;
      text-align: center;
      margin: 25px auto;
      padding: 12px 30px;
      color: @brand-primary;
      border-color: @brand-primary;
    }
    #star {
      background: @brand-primary;
      width: 100px;
      height: 100px;
      position: relative;
      text-align: center;
      color: white;
      span{
        position: relative;
        display: block;
        z-index: 10;
      }
      span:nth-child(1){
        font-size: 22px;
        position: relative;
        top: 10px;
      }
      span:nth-child(2){
        font-size: 54px;
        font-weight: 400;
      }
    }
    #star:before, #star:after {
      content: "";
      position: absolute;
      top: 0;
      left: 0;
      height: 100px;
      width: 100px;
      background: @brand-primary;
    }
    #star:before {
      -webkit-transform: rotate(30deg);
      -moz-transform: rotate(30deg);
      -ms-transform: rotate(30deg);
      -o-transform: rotate(30deg);
    }
    #star:after {
      -webkit-transform: rotate(60deg);
      -moz-transform: rotate(60deg);
      -ms-transform: rotate(60deg);
      -o-transform: rotate(60deg);
    }
  }

}

