.custom-modal.online{
  overflow-y: scroll;
  #send{
    margin-top: 30px;
    @media screen and (max-width: @screen-sm-max) {
      margin: 5vw auto !important;
    }
  }
  .choose-special-price.individual{
    visibility: hidden;
  }
  .verbose{
    h2{
      margin: 30px 0;
      padding: 20px 0;
      color: white;
      -webkit-background-size: cover;
      background-size: cover;
      text-shadow: 0 0 3px rgba(0, 0, 0, .7);
      &.standard{
        background: @gray;
      }
      &.gold{
        background: @brand-primary;
      }
      &.platinum, &.individual{
        background: @brand-primary;
      }
      &.vip{
        background: @brand-primary;
      }
    }
  }
}


.class-overlay{

  position: fixed;
  z-index: @zindex-modal;
  background: rgba(255, 255, 255, .8);
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  opacity: 0;
  visibility: hidden;
  .transition(.4s);

  .container{
    margin-top: 50px;
    background: white;
    padding-bottom: 30px;
    box-shadow: 0 0 10px rgba(0, 0, 0, .3);
    border-radius: 5px;
    .translate(0, -100%);
    .transition(.4s);
    opacity: 0;
    @media screen and (max-width: @screen-sm-max) {
      height: 100%;
      margin-top: 0;
      border-radius: 0;
      background: rgba(255, 255, 255, .7);
      overflow-y: scroll;
    }
  }

  &._opened{
    opacity: 1;
    visibility: visible;
    .container{
      .translate(0, 0);
      opacity: 1;
    }
  }

  .close{
    position: absolute;
    top: 20px;
    right: 20px;
    font-size: 40px;

  }

  h3{
    margin: 40px;
  }

  .row{

    div{
      .make-md-column(6);
      &:nth-child(1){
        img{
          width: 100%;
          border: 1px solid white;
        }
      }
      &:nth-child(2){
        p{
          line-height: 1.3em;
        }
        .quot{
          font-size: 16px;
          font-weight: 400;
        }
        @media screen and (max-width: @screen-sm-max) {
          margin-top: 20px;
        }
      }
    }
  }

}

.pac-container{
  z-index: @zindex-dropdown;
}

.reviews-link{
  position: fixed;
  width: 400px;
  right: 20px;
  top: 50%;
  .translate(0, -50%);
  background: fade(@brand-primary, 90%);
  text-align: center;
  box-shadow: 0 0 20px rgba(0, 0, 0, .3);
  z-index: @zindex-modal-background - 1;
  cursor: pointer;
  .heading{
    text-align: center;
    font-family: @font-family-serif;
    text-align: center;
    padding: 20px 25px;
    color: white;
    font-size: 22px;
    .close-it{
      position: absolute;
      right: 10px;
    }
  }
  .body{
    padding: 10px;
  }
  @media screen and (max-width: @screen-sm-max) {
    width: 70%;
    right: 0;
    //top: auto;
    //.translate(0, 0);
    //bottom: 0;
    .heading{
      font-size: 14px;
      padding: 20px 10px;
      text-align: left;
      .close-it{
        top: 2px;
        left: auto;
        right: -25px;
        font-size: 35px;
      }
    }
  }
}

.video-about{

  padding: 20px 0 40px 0;

  .container .row .col-md-10{
    padding-left: 10px;
  }

}

.reviews-lead{
  text-align: center;
  margin-bottom: 50px;
  &.dod{
